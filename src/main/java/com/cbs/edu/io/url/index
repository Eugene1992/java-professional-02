.1/jquery.cookie.min.js"></script>        <script>            $(document).ready(function() {                var url = window.location.href;                var isError = url.endsWith("error");                if(isError){                    $("#login-dropdown-btn").dropdown('toggle');                }            });            $(document).ready(function() {                var lang = window.location.href.split('?lang=')[1];                if ($.cookie('lang') === null){                    $.cookie("lang", "ENG");                } else {                    if (lang === 'ru' || $.cookie('lang') === 'ru') {                        $.cookie("lang", "RUS");                        $("#lang").text(lang = $.cookie("lang"));                    } else {                        $.cookie("lang", "ENG");                        $("#lang").text(lang = $.cookie("lang"));                    }                }                /*$("#lang").text(lang === 'ru' ? "RUS" : "ENG");*/            });        </script>    </body></html>